{
  "expo": {
    "name": "ChawpVendor",
    "slug": "chawpvendor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/vendor_logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/vendor_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stephen_j.ChawpVendor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/vendor_logo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.stephen_j.ChawpVendor",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "POST_NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/vendor_logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "aacc20b5-b2df-4ebb-93c7-44c12c3139d1"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/vendor_logo.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ]
  }
}
